<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <context-param>
        <param-name>contextConfigLocation</param-name>
        <param-value>/WEB-INF/applicationContext.xml</param-value>
    </context-param>
    <listener>
        <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>dispatcher</servlet-name>
        <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
        <load-on-startup>2</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>s1</servlet-name>
        <servlet-class>Servlets.s1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>s2</servlet-name>
        <servlet-class>Servlets.s2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>s3</servlet-name>
        <servlet-class>Servlets.s3</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>s4</servlet-name>
        <servlet-class>Servlets.s4</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>s5</servlet-name>
        <servlet-class>Servlets.s5</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>s6</servlet-name>
        <servlet-class>Servlets.s6</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>s7</servlet-name>
        <servlet-class>Servlets.s7</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>s8</servlet-name>
        <servlet-class>Servlets.s8</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>s9</servlet-name>
        <servlet-class>Servlets.s9</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dispatcher</servlet-name>
        <url-pattern>*.htm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>s1</servlet-name>
        <url-pattern>/s1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>s2</servlet-name>
        <url-pattern>/s2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>s3</servlet-name>
        <url-pattern>/s3</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>s4</servlet-name>
        <url-pattern>/s4</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>s5</servlet-name>
        <url-pattern>/s5</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>s6</servlet-name>
        <url-pattern>/s6</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>s7</servlet-name>
        <url-pattern>/s7</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>s8</servlet-name>
        <url-pattern>/s8</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>s9</servlet-name>
        <url-pattern>/s9</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>redirect.jsp</welcome-file>
    </welcome-file-list>
</web-app>
